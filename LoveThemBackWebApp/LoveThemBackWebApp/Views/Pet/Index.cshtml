@model List<LoveThemBackWebApp.Models.Pet>

@{
  Layout = "_Layout";
}

<table>

  @foreach (var pet in Model)
  {

  <tr>
    <td>
    <a asp-action="Details" asp-route-id="@pet.id.tspo">Details</a>
      </td>
    <td>
      @Html.DisplayFor(modelItem => pet.id.tspo)
    </td>
    <td>
      @Html.DisplayFor(modelItem => pet.name.tsju)
    </td>
    <td>
      @Html.DisplayFor(modelItem => pet.age.tsna)
    </td>
    <td>
      @Html.DisplayFor(modelItem => pet.description.tsnve)
    </td>
  </tr>
  }
</table>
